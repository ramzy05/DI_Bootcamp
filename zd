[32m+[m
[32m+[m
[32m+[m[32m//<head>[m
[32m+[m[32mfunction getInput(){[m
[32m+[m[32m  let input;[m
[32m+[m[32m  do{[m
[32m+[m[32m    input = prompt('Enter words separate by comma');[m
[32m+[m[32m    if(input instanceof String || !input/* to make sur the string is empty */){alert("Please enter a sequence of words");}[m
[32m+[m[32m  }while(!input);[m
[32m+[m[32m  input = input.replace(/ /gm, '');//remove whitespace[m
[32m+[m[32m  return input.split(',');[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction theWidthAndHeight(liste){[m
[32m+[m[32m  let maxLengthItem = liste[0].length[m
[32m+[m[32m  for(let i = 1; i<liste.length; i++){[m
[32m+[m[32m    if(maxLengthItem < liste[i].length){[m
[32m+[m[32m      maxLengthItem = liste[i].length[m
[32m+[m[32m    }[m
[32m+[m[32m  }//width = maxLenfthItem + 2, height = liste.lenght + 2[m
[32m+[m[32m  return [maxLengthItem + 4, liste.length + 2];[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction draw(){[m
[32m+[m[32m  let liste = getInput()[m
[32m+[m[32m  console.log(liste)[m
[32m+[m[32m  let width_height = theWidthAndHeight(liste);[m
[32m+[m[32m  //print the first line of star[m
[32m+[m[32m  for(let i = 0; i < width_height[0]; i++){[m
[32m+[m[32m    document.write(`*`);[m
[32m+[m[32m  }[m
[32m+[m[32m  document.write(`<br>`);[m
[32m+[m[32m  for(let i = 0; i <= width_height[1] - 3; i++){[m
[32m+[m[32m    document.write(`*&nbsp;&nbsp;${liste[i]}`);[m
[32m+[m[32m    /* for(let j=0; j < Math.round(((width_height[0]-(liste[i-1]).length))/2); j++){[m
[32m+[m[32m      console.log(Math.round(((width_height[0]-(liste[i-1]).length))/2));[m
[32m+[m[32m      document.write(``);[m
[32m+[m[32m    } */[m
[32m+[m[32m    /* document.write(`&nbsp;${liste[i-1]}`); */[m
[32m+[m[32m    if(liste[i].length == width_height[0]-4){[m
[32m+[m[32m      for(let j=1; j <= ((width_height[0]-liste[i].length-1)); j++){[m
[32m+[m[32m        document.write(`&nbsp;`);/*  */[m
[32m+[m[32m      }[m
[32m+[m[32m    }else{[m
[32m+[m
[32m+[m[32m      for(let j=1; j <= ((width_height[0] -liste[i].length)+2); j++){[m
[32m+[m[32m        document.write(`&nbsp;`);/*  */[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    console.log(width_height[0]-liste[i].length);[m
[32m+[m[32m    document.write(`*<br>`);[m
[32m+[m[32m  }[m
[32m+[m[32m   //print the last line of star[m
[32m+[m[32m  for(let i = 0; i < width_height[0]; i++){[m
[32m+[m[32m    document.write(`*`);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
