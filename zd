[1mdiff --git a/week4/day5/exercises/ninja/calendarGenerator.html b/week4/day5/exercises/ninja/calendarGenerator.html[m
[1mindex e69de29..77f00c6 100644[m
[1m--- a/week4/day5/exercises/ninja/calendarGenerator.html[m
[1m+++ b/week4/day5/exercises/ninja/calendarGenerator.html[m
[36m@@ -0,0 +1,52 @@[m
[32m+[m[32m<!DOCTYPE html>[m
[32m+[m[32m<html lang="en">[m
[32m+[m[32m<head>[m
[32m+[m[32m  <meta charset="UTF-8">[m
[32m+[m[32m  <meta http-equiv="X-UA-Compatible" content="IE=edge">[m
[32m+[m[32m  <meta name="viewport" content="width=device-width, initial-scale=1.0">[m
[32m+[m[32m  <title>Document</title>[m
[32m+[m[32m</head>[m
[32m+[m[32m<body>[m
[32m+[m[32m  <tr></tr>[m
[32m+[m
[32m+[m[32m  <script>[m
[32m+[m[32m    function createCalendar(year, month){[m
[32m+[m[32m      function getDay(date){[m
[32m+[m[32m        //this function is to set sunday as the last day[m
[32m+[m[32m        let day = date.getDay();[m
[32m+[m[32m        if(day == 0/*Sunday*/)/*then*/ day = 7;[m
[32m+[m[32m        return day - 1; // day in range 0-7 :-)[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      let table = "<table><tr><th>MO</th>\[m
[32m+[m[32m        <th>TU</th><th>WE</th><th>TH</th>\[m
[32m+[m[32m        <th>FR</th><th>SA</th><th>SU</th></tr><tr>";[m[41m [m
[32m+[m[32m      let goodMonth = month -1;[m
[32m+[m[32m      let date = new Date(year, goodMonth);[m
[32m+[m[32m      /*date.getDay() return a number in range 0-6, which matches sund-friday[m
[32m+[m[32m      and that number is the first the day of the month[m
[32m+[m[32m      so will should put white space on before the month first day*/[m
[32m+[m[32m      for (let index = 0; index < getDay(date); index++) {[m
[32m+[m[32m        table += '<td>.</td>';[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      //let'us add the month's days starting by the first day[m
[32m+[m[32m      while(date.getMonth() == goodMonth){[m
[32m+[m[32m        table += `<td>${date.getDate()}</td>`;[m
[32m+[m
[32m+[m[32m        //check if the day is sunday next row[m
[32m+[m[32m        if(getDay(date) === 6){[m
[32m+[m[32m          table = '</tr><tr>';[m
[32m+[m[32m        }[m
[32m+[m[32m        //increment the day using setDay[m
[32m+[m[32m        date.setDate(date.getDate() + 1 );[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      table += '</tr></table>';[m
[32m+[m[32m      document.body.innerHTML = table;[m
[32m+[m[32m     //document.querySelector('table').style.border = '1px solid red';[m
[32m+[m[32m    }[m
[32m+[m[32m    createCalendar(2012, 9);[m
[32m+[m[32m  </script>[m
[32m+[m[32m</body>[m
[32m+[m[32m</html>[m
\ No newline at end of file[m

