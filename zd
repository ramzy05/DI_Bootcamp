[32m+[m[32m  (function infoAboutMe(){[m
[32m+[m[32m    console.log('My name is Ramses, my hobbies are football and coding');[m
[32m+[m[32m  }());[m
[32m+[m[32m//endI[m
[32m+[m
[32m+[m[32m//partII[m
[32m+[m[32m  function infoAboutPerson(personName, personAge, personFavoriteColor){[m
[32m+[m[32m    console.log(`Your name is ${personName}, you are ${personAge} years old, your favorite color is ${personFavoriteColor}`);[m
[32m+[m[32m  }[m
[32m+[m[32m  infoAboutPerson("David", 45, "blue");[m
[32m+[m[32m  infoAboutPerson("Josh", 12, "yellow");[m
[32m+[m
[32m+[m[32m//endII[m
[32m+[m
[32m+[m[32m//end exo1[m
[32m+[m[32m//exo 2[m
[32m+[m
[32m+[m[32mfunction calculateTip(){[m
[32m+[m[32m  let billAmount = parseFloat(prompt('Enter the amount of the bill'));[m
[32m+[m[32m  let tipAmount ;[m
[32m+[m[32m  if(billAmount < 50){//<head>[m
[32m+[m[32m    tipAmount = parseFloat(((billAmount * 20)/100).toFixed(2))[m
[32m+[m[32m  }else if(billAmount >= 50 && billAmount <= 200){[m
[32m+[m[32m    tipAmount = parseFloat(((billAmount * 15)/100).toFixed(2))[m[41m [m
[32m+[m[32m  }else{[m
[32m+[m[32m    tipAmount = parseFloat(2((billAmount * 10)/100).toFixed(2))[m[41m [m
[32m+[m[32m  }[m
[32m+[m[32m  //final bill[m
[32m+[m[32m  console.log(`Your final bill is \$${billAmount + tipAmount}`);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/* calculateTip(); */[m
[32m+[m
[32m+[m[32m//end exo2[m
[32m+[m
[32m+[m[32m//exo 3[m
[32m+[m[32m  function isDivisible(divisor){[m
[32m+[m[32m    let sumOfDivisibleNumber = 0;[m
[32m+[m[32m    console.log('Outcome : ');[m
[32m+[m[32m    for(let i = 0; i < 501; i++){[m
[32m+[m[32m      if(i % divisor === 0){[m
[32m+[m[32m        sumOfDivisibleNumber += i;[m
[32m+[m[32m        console.log(`${i} `)[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    console.log(`Sum : ${sumOfDivisibleNumber}`);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m/* isDivisible(23); */[m
[32m+[m[32m//end exo3[m
[32m+[m
[32m+[m
[32m+[m[32m//exo 4[m
[32m+[m
[32m+[m[32m  let stock = {[m[41m [m
[32m+[m[32m    "banana": 6,[m[41m [m
[32m+[m[32m    "apple": 0,[m
[32m+[m[32m    "pear": 12,[m
[32m+[m[32m    "orange": 32,[m
[32m+[m[32m    "blueberry":1[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  let prices = {[m[41m    [m
[32m+[m[32m    "banana": 4,[m[41m [m
[32m+[m[32m    "apple": 2,[m[41m [m
[32m+[m[32m    "pear": 1,[m
[32m+[m[32m    "orange": 1.5,[m
[32m+[m[32m    "blueberry":10[m
[32m+[m[32m  };[m[41m [m
[32m+[m
[32m+[m[32m  let shoppingList = ['banana', 'orange', 'apple'];[m
[32m+[m
[32m+[m[32m  function myBill(){[m
[32m+[m[32m    let totalPrice = 0;[m
[32m+[m[32m    for(let name of shoppingList){[m
[32m+[m[32m      if(stock[name] > 0){[m
[32m+[m[32m        //decrease the stock by 1[m
[32m+[m[32m        --stock[name];[m
[32m+[m[32m        //add the price of item to totalPrice[m
[32m+[m[32m        totalPrice += prices[name];[m
[32m+[m
[32m+[m[32m      }[m
[32m+[m[32m   }[m
[32m+[m[32m   return totalPrice;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  myBill();[m
[32m+[m[32m//end exo4[m
[32m+[m
[32m+[m
[32m+[m[32m//exo 5//<head>[m
[32m+[m
[32m+[m[32mfunction changeEnough(itemPrice, amountOfChange){[m
[32m+[m[32m  return([m
[32m+[m[32m    itemPrice <= amountOfChange[0]*0.25 +[m[41m [m
[32m+[m[32m                amountOfChange[1]*0.10 +[m
[32m+[m[32m                amountOfChange[2]*0.05 +[m
[32m+[m[32m                amountOfChange[3]*0.01[m
[32m+[m[32m  );[m
[32m+[m[41m [m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mchangeEnough(4.25, [25, 20, 5, 0])[m
[32m+[m[32m//end exo5[m
[32m+[m
[32m+[m
[32m+[m[32m//exo 6//<head>[m
[32m+[m
[32m+[m[32mfunction hotelCost(day){[m
[32m+[m[32m  //here we are sure that day is a positive number[m
[32m+[m[32m  return day * 140;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction planeRideCost(destination){[m
[32m+[m[32m  //the destination is not and empy string[m
[32m+[m[32m  if(destination === 'London' || destination.toLowerCase() === 'london'){[m
[32m+[m[32m    return 183;[m
[32m+[m[32m  }else if (destination === 'Paris' || destination.toLowerCase() === 'paris'){[m
[32m+[m[32m    return 220;[m
[32m+[m[32m  }else{[m
[32m+[m[32m    return 300;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mfunction rentalCarCost(day){[m
[32m+[m[32m  if(day > 10){[m
[32m+[m[32m    return (day * 40) - (day*40)*5/100[m
[32m+[m[32m  }[m
[32m+[m[32m  return day*40[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mfunction totalVacationCost(){[m
[32m+[m[32m  let dayHotelCost, destination ,dayCarCost;[m
[32m+[m[32m  do{[m
[32m+[m[32m    dayHotelCost = parseInt(prompt('Enter the number of day you are going to stay in the hotel'));[m
[32m+[m[32m    if(typeof(dayHotelCost) == undefined || isNaN(dayHotelCost) || dayHotelCost < 0){alert("Please enter a correct positive number!");}[m
[32m+[m[32m  }while(isNaN(dayHotelCost) || dayHotelCost < 0);[m
[32m+[m
[32m+[m
[32m+[m[32m  do{[m
[32m+[m[32m    dayCarCost = parseInt(prompt('Enter the number of days you would like to rent the car'));[m
[32m+[m[32m    if(typeof(dayCarCost) == undefined || isNaN(dayCarCost) || dayCarCost < 0){alert("Please enter a correct positive number!");}[m
[32m+[m[32m  }while(isNaN(dayCarCost) || dayCarCost < 0);[m
[32m+[m
[32m+[m[32m  do{[m
[32m+[m[32m    destination = prompt('Enter your destination');[m
[32m+[m[32m    if(destination instanceof String || !destination /* to make sur the string is empty */){alert("Please enter a correct destination");}[m
[32m+[m[32m  }while(!destination) ;[m
[32m+[m
[32m+[m[32m  console.log(`The car cost: \$${rentalCarCost(dayCarCost)}, \[m
[32m+[m[32mthe hotel cost: \$${hotelCost(dayHotelCost)}, \[m
[32m+[m[32mthe plane tickets cost: ${planeRideCost(destination)}.`);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m//end exo6//<head>[m
\ No newline at end of file[m

