[32m+[m[32m//functions<>[m
[32m+[m[32mlet expr = '';[m
[32m+[m
[32m+[m[32mlet number = (num)=>{[m
[32m+[m[32m  expr += num;[m
[32m+[m[32m  console.log(expr);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mlet operator = (operator)=>{[m
[32m+[m[32m  expr += operator;[m
[32m+[m[32m  console.log(expr);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mlet equal = ()=>{[m
[32m+[m[32m  if(eval()){[m
[32m+[m[32m    let calcTable = [];[m
[32m+[m[32m    count = -1;[m
[32m+[m[32m    for(let i = 0; i < expr.length; i++){[m
[32m+[m[32m      count++;[m
[32m+[m[32m      calcTable.push('')[m
[32m+[m[32m      if((/[\d]+/).test(expr[i])){[m
[32m+[m[32m        calcTable[count] += expr[i];[m
[32m+[m[32m      }[m
[32m+[m[32m      else{[m
[32m+[m[32m        calcTable[count] = parseInt(calcTable[count])[m
[32m+[m[32m        calcTable.push(expr[i]);[m
[32m+[m[32m        count ++;[m
[32m+[m[32m        continue;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    for(index in calcTable ){[m
[32m+[m[32m      if(calcTable[index] === '' || isNaN(calcTable[index])){[m
[32m+[m[32m        calcTable.splice(index,1)[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    //concatenate number[m
[32m+[m[32m    console.log(calcTable);[m
[32m+[m[32m    let operand = '';[m
[32m+[m[32m    let tmp = [];[m
[32m+[m[32m    for(let i = 0;  i < calcTable.length+1 ; i++){[m
[32m+[m[41m      [m
[32m+[m[32m      if((/[\d]+/).test(calcTable[i])){[m
[32m+[m[32m        operand += calcTable[i][m
[32m+[m[32m      }else{[m
[32m+[m[32m        tmp.push(parseInt(operand))[m
[32m+[m[32m        tmp.push(calcTable[i])[m
[32m+[m[32m        operand = ''[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    tmp.pop();[m
[32m+[m[32m    calcTable = tmp;[m
[32m+[m
[32m+[m[32m    //end concatenate number[m
[32m+[m[32m    //calulate now[m
[32m+[m[32m    let result = calcTable[0][m
[32m+[m[32m    for(let i = 0;  i < calcTable.length-1 ; i += 2){[m
[32m+[m[32m      switch (calcTable[i+1]) {[m
[32m+[m[32m        case '+':[m
[32m+[m[32m          result += calcTable[i+2][m
[32m+[m[32m          break;[m
[32m+[m[32m        case '-':[m
[32m+[m[32m          result -= calcTable[i+2][m
[32m+[m[32m          break;[m
[32m+[m[32m        case '*':[m
[32m+[m[32m          result *= calcTable[i+2][m
[32m+[m[32m          break;[m
[32m+[m[32m        case '/':[m
[32m+[m[32m          result /= calcTable[i+2][m
[32m+[m[32m          break;[m
[32m+[m[41m      [m
[32m+[m[32m        default:[m
[32m+[m[32m          break;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    let globalResult ='';[m
[32m+[m[32m    calcTable.forEach(element => {[m
[32m+[m[32m      globalResult += element[m
[32m+[m[32m    });[m
[32m+[m[32m    alert(` = ${result}`);[m
[32m+[m[32m    expr = ''[m
[32m+[m[32m    //calulate now[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mlet eval = ()=>{[m
[32m+[m[32m  if(expr.match(/^[\+\-\+\*\\][\d]+$/)){[m
[32m+[m
[32m+[m[32m    alert(`${expr} is invalid operation`);[m
[32m+[m[32m    return false;[m
[32m+[m[32m  }[m
[32m+[m[32m  return true;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// End functions[m
