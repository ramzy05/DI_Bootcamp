[32m+[m[32m                                                      7[m
[32m+[m
[32m+[m[32m                       CSS                               fl:r[m
[32m+[m
[32m+[m[32mCSS module uses fuzzy search to find unknown abb-             float:right;[m
[32m+[m[32mreviations, e.g. ov:h == ov-h == ovh == oh.[m
[32m+[m[32m                                                         cl[m
[32m+[m[32mIf abbreviation wasnât found, it is transformed into[m
[32m+[m[32mproperty name: foo-bar â foo-bar: |;                          clear:both;[m
[32m+[m
[32m+[m[32mYou can prefix abbreviations with hyphen to produce      cl:n[m
[32m+[m[32mvendor-prefixed properties: -foo[m
[32m+[m[32m                                                              clear:none;[m
[32m+[m[32m Visual Formatting[m
[32m+[m[32m                                                         cl:l[m
[32m+[m[32mpos[m
[32m+[m[32m                                                              clear:left;[m
[32m+[m[32m     position:relative;[m
[32m+[m[32m                                                         cl:r[m
[32m+[m[32mpos:s[m
[32m+[m[32m                                                              clear:right;[m
[32m+[m[32m     position:static;[m
[32m+[m[32m                                                         cl:b[m
[32m+[m[32mpos:a[m
[32m+[m[32m                                                              clear:both;[m
[32m+[m[32m     position:absolute;[m
[32m+[m[32m                                                         d[m
[32m+[m[32mpos:r[m
[32m+[m[32m                                                              display:block;[m
[32m+[m[32m     position:relative;[m
[32m+[m[32m                                                         d:n[m
[32m+[m[32mpos:f[m
[32m+[m[32m                                                              display:none;[m
[32m+[m[32m     position:fixed;[m
[32m+[m[32m                                                         d:b[m
[32m+[m[32mt[m
[32m+[m[32m                                                              display:block;[m
[32m+[m[32m     top:;[m
[32m+[m[32m                                                         d:f[m
[32m+[m[32mt:a[m
[32m+[m[32m                                                              display:flex;[m
[32m+[m[32m     top:auto;[m
[32m+[m[32m                                                         d:if[m
[32m+[m[32mr[m
[32m+[m[32m                                                              display:inline-flex;[m
[32m+[m[32m     right:;[m
[32m+[m[32m                                                         d:i[m
[32m+[m[32mr:a[m
[32m+[m[32m                                                              display:inline;[m
[32m+[m[32m     right:auto;[m
[32m+[m[32m                                                         d:ib[m
[32m+[m[32mb[m
[32m+[m[32m                                                              display:inline-block;[m
[32m+[m[32m     bottom:;[m
[32m+[m[32m                                                         d:li[m
[32m+[m
[32m+[m[32m                                                              display:list-item;[m
[32m+[m
[32m+[m[32mb:a                                                      d:ri[m
[32m+[m
[32m+[m[32m     bottom:auto;                                             display:run-in;[m
[32m+[m
[32m+[m[32ml                                                        d:cp[m
[32m+[m
[32m+[m[32m     left:;                                                   display:compact;[m
[32m+[m
[32m+[m[32ml:a                                                      d:tb[m
[32m+[m
[32m+[m[32m     left:auto;                                               display:table;[m
[32m+[m
[32m+[m[32mz                                                        d:itb[m
[32m+[m
[32m+[m[32m     z-index:;                                                display:inline-table;[m
[32m+[m
[32m+[m[32mz:a                                                      d:tbcp[m
[32m+[m
[32m+[m[32m     z-index:auto;                                            display:table-caption;[m
[32m+[m
[32m+[m[32mfl                                                       d:tbcl[m
[32m+[m
[32m+[m[32m     float:left;                                              display:table-column;[m
[32m+[m
[32m+[m[32mfl:n                                                     d:tbclg[m
[32m+[m
[32m+[m[32m     float:none;                                              display:table-column-group;[m
[32m+[m
[32m+[m[32mfl:l                                                     d:tbhg[m
[32m+[m
[32m+[m[32m     float:left;                                              display:table-header-group;[m
[32m+[m[32md:tbfg                            8[m
[32m+[m
[32m+[m[32m     display:table-footer-group;    ovx:a[m
