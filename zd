[32m+[m[32mlet computerNumber = Math.floor(Math.random()*10);[m
[32m+[m
[32m+[m[32m//<head>[m
[32m+[m[32mlet playTheGame = ()=>{[m
[32m+[m
[32m+[m[32m  alert(computerNumber);[m
[32m+[m[32m  if(confirm('Do you want to play game?')){[m
[32m+[m[32m    let userInput, count = 1;[m
[32m+[m[41m    [m
[32m+[m[32m    do{[m
[32m+[m[41m      [m
[32m+[m[32m       userInput = parseInt(prompt('Please enter a number'));[m
[32m+[m[32m      if (isNaN(userInput)){[m
[32m+[m[32m        alert('Sorry Not a number');[m
[32m+[m[32m      }else{[m
[32m+[m[32m        if(!(0 <= userInput && userInput <=10)){[m
[32m+[m[32m          alert('Sorry it\âs not a good number');[m
[32m+[m[32m          count ++;[m
[32m+[m[32m        }else{[m
[32m+[m[41m          [m
[32m+[m[32m          if(test(userInput, computerNumber)){[m
[32m+[m[32m            break;[m
[32m+[m[32m          }else{[m
[32m+[m[32m            count ++;[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m      if(count === 4){[m
[32m+[m[32m        alert('out of chances')[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m[32m    }while(count < 4 );[m
[32m+[m[32m    return;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  alert('No problem, Goodbye');[m
[32m+[m[32m  return;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mlet test = (userNumber,computerNumber)=>{[m
[32m+[m[41m  [m
[32m+[m[32m  if(userNumber < computerNumber){[m
[32m+[m[32m    alert ('Your number is smaller then the computer\âs, guess again')[m
[32m+[m[32m    return false;[m
[32m+[m[32m  }else if(userNumber > computerNumber){[m
[32m+[m[32m    alert("Your number is bigger then the computerâs, guess again")[m
[32m+[m[32m    return false;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  alert('WINNER')[m
[32m+[m[32m  return true;[m
[32m+[m[41m [m
[32m+[m[32m}[m
