[32m+[m[32m  height : 400px;[m
[32m+[m[32m  border: 3px solid red;[m
[32m+[m[32m  display:flex;[m[41m [m
[32m+[m[32m  /* display:flex on the parent container */[m
[32m+[m[32m  flex-direction: row;[m
[32m+[m[32m  /* flex-direction: row DEFAULT */[m
[32m+[m[32m  justify-content:space-around;[m
[32m+[m[32m  align-items: flex-start;[m
[32m+[m[32m/*   align items in the container along the cross axis (y axis).[m[41m [m
[32m+[m[32m  DEFAULT STRECH */[m
[32m+[m[32m  flex-wrap:wrap;[m
[32m+[m[32m  align-content: space-between;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.div1 {[m
[32m+[m[32m  border: 1px solid blue;[m
[32m+[m[32m  background-color: #e6ffff;[m
[32m+[m[32m  width: 50%;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.div2 {[m
[32m+[m[32m  border: 1px solid red;[m
[32m+[m[32m  background-color: #ffd6cc;[m
[32m+[m[32m  width: 30%;[m
[32m+[m[32m/* See what happens if width: 80%;   */[m
[32m+[m[32m  align-self: flex-end[m
[32m+[m[32m}[m
