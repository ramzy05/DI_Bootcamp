[32m+[m[32m    <span id="story"></span>[m
[32m+[m[32m    </p>[m
[32m+[m[32m    <script>[m
[32m+[m[32m      let count = 0;[m
[32m+[m[32m      let sentence = [], output = '';[m
[32m+[m[32m      const libBtn = document.getElementById('lib-button');[m
[32m+[m[32m      const shuflBtn = document.getElementById('shufl-button');[m
[32m+[m[32m      const inputs  = document.querySelectorAll('input');[m
[32m+[m[32m      libBtn.addEventListener('click',function(e){[m
[32m+[m[32m        sentence = [];[m
[32m+[m[32m        count = 0;[m
[32m+[m[32m        output = ''[m
[32m+[m[32m        inputs.forEach(function(input){[m
[32m+[m[32m          if(input.value == ''){[m
[32m+[m[32m            alert(input.getAttribute('id')+' field cannot be blank');[m
[32m+[m[32m          }else{[m
[32m+[m[32m            sentence.push(input.value)[m
[32m+[m[32m          }[m
[32m+[m[32m        });[m
[32m+[m[32m        if(sentence){[m
[32m+[m[32m          count ++;[m
[32m+[m[32m          console.log(sentence.join(' '));[m
[32m+[m[32m        }[m
[32m+[m[32m        e.preventDefault();[m
[32m+[m[32m      });[m
[32m+[m[41m      [m
[32m+[m[32m      function shuffle(array) {[m
[32m+[m[32m        return array.sort(() => Math.random() - 0.5);[m
[32m+[m[32m      }[m
[32m+[m[32m      shuflBtn.addEventListener('click', function(e){[m
[32m+[m[32m          if(count >= 1 && 3 >= count && sentence){[m
[32m+[m[32m            sentence = shuffle(sentence);[m
[32m+[m[32m            console.log(sentence.join(' '));[m
[32m+[m[32m            count++;[m[41m [m
[32m+[m[32m          }else{[m
[32m+[m[32m            count = 0;[m
[32m+[m[32m            inputs.forEach(function(input){[m
[32m+[m[32m              input.value = '';[m
[32m+[m[32m            });[m
[32m+[m[32m          if(sentence == []){[m
[32m+[m[32m            sentence = [];[m
[32m+[m[32m            console.log("Please enter new inputs")[m
[32m+[m[32m          }else{[m
[32m+[m[32m          console.log("Please enter inputs")[m
[32m+[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m          e.preventDefault()[m
[32m+[m[32m        });[m
[32m+[m[32m    </script>[m
[32m+[m[32m</body>[m
[32m+[m[32m</html>[m
